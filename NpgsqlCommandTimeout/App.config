      </dependentAssembly>-->
      <dependentAssembly>
        <assemblyIdentity name="Npgsql.EntityFramework" culture="neutral" />
        <bindingRedirect oldVersion="2.0.0.0-2.0.14.1" newVersion="2.0.14.2" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Npgsql" publicKeyToken="5d8b90d52f46fda7" culture="neutral" />
